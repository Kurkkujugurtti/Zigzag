Operator {
   identifier: String
   priority: num

   init(identifier, priority) {
      this.identifier = identifier
      this.priority = priority
   }
}

Parser {
   operators = List(Operator)
}

Assembler {

}

Compiler {
   parser = Parser()
   assembler = Assembler()
}

init(compiler: Compiler) {
   power_operator = Operator('^', 18)
   compiler.parser.operators.add(power_operator)
   
}