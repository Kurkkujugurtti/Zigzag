readln() {
	buffer = allocate(256)
	length = sys_read(buffer, 256) - 2

	if length <= 0 {
		free(buffer)
		=> String('')   
	}

	result = allocate(length)
	copy(buffer, length, result)
	free(buffer)

	=> String(result)
}

prints(text: String) {
	sys_print(text.data(), text.length())
}

print(text: link) {
	sys_print(text, length_of(text))
}

printsln(text: String) {
	sys_print(text.append(10).data(), text.length() + 1)
}

println(text: link) {
	prints(String(text).append(10))
}