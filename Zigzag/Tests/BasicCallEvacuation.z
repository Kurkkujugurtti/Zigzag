import large_function()

export basic_call_evacuation(a, b) {
    c = a * b + 10
    large_function()
    => a + b + c
}

export basic_call_evacuation_with_memory(a, b) {
    c = a * b + 10
    d = a * b + 10
    e = a * b + 10
    large_function()
    => a + b + c + d + e
}

init() {
	=> 1
    
    # Dummy for type resolvation
    basic_call_evacuation(1, 1)
    basic_call_evacuation_with_memory(1, 1)
}