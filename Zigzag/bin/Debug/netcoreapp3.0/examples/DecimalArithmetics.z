basic_calculation(a, b) {
    => a + b - a * b / (1.0 + a) + 3.14159
}

decimal_array_test(a, b, c) {
    array = decimal[(a + c) as num]
    array[a as num] = b + c
    => array[(b + 7) as num] + c
}

init() {
    a = 100.0
    b = 700.0
    c = basic_calculation(a, b)
    d = decimal_array_test(a, b, c)
    => a + b + c + d
}